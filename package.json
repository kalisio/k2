{
  "name": "k2",
  "description": "3D terrain server for Cesium tiles",
  "version": "0.3.0",
  "main": "index.js",
  "bin": "index.js",
  "homepage": "https://github.com/kalisio/k2",
  "keywords": [
    "kargo",
    "cesium"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
